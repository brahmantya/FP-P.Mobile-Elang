<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.workshopkotlin.anggitprayogo">

    <application
            android:name=".App"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".feature.CheckoutActivity">
        </activity>
        <activity android:name=".feature.DetailProductActivity">
        </activity>
        <activity android:name=".feature.ProductActivity">
        </activity>
        <activity android:name=".feature.RegisterActivity">
        </activity>
        <activity android:name=".feature.LoginActivity">
        </activity>
        <activity android:name=".feature.CreateProductActivity">
        </activity>
        <activity android:name=".feature.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".feature.MainActivity"/>
    </application>

</manifest>